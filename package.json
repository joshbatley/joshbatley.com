{
  "name": "josh-batley.com",
  "version": "1.0.0",
  "repository": "git@github.com:jbatley/josh-batley.com.git",
  "author": "Josh Batley <jbatley12@gmail.com>",
  "license": "MIT",
  "browserslist": [
    "> 1%",
    "not dead"
  ],
  "devDependencies": {
    "@parcel/optimizer-data-url": "^2.13.3",
    "@parcel/transformer-inline-string": "^2.13.3",
    "parcel": "^2.13.3"
  },
  "scripts": {
    "dev": "parcel src/index.html --dist-dir ./_site",
    "build": "pnpm build-site && pnpm move-meta",
    "build-site": "parcel build src/*.html --no-source-maps --dist-dir ./_site",
    "move-meta": "cp ./meta/* ./_site/",
    "serve": "serve -p 8080 -s ./_site"
  }
}