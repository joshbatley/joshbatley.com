{
  "name": "joshbatley.com",
  "version": "1.0.0",
  "repository": "git@github.com:jbatley/joshbatley.com.git",
  "author": "Josh Batley <jbatley12@gmail.com>",
  "license": "MIT",
  "browserslist": [
    "> 1%",
    "not dead"
  ],
  "devDependencies": {
    "@parcel/optimizer-data-url": "^2.13.3",
    "@parcel/transformer-inline-string": "^2.13.3",
    "date-fns": "^4.1.0",
    "parcel": "^2.13.3",
    "showdown": "^2.1.0"
  },
  "scripts": {
    "dev": "parcel src/*.html --dist-dir ./_site",
    "build": "pnpm build-blog && pnpm build-site && pnpm move-meta",
    "build-site": "parcel build src/*.html --no-source-maps --dist-dir ./_site",
    "move-meta": "cp ./meta/* ./_site/",
    "serve": "serve -p 8080 -S ./_site",
    "new-post": "node ./scripts/new-post.js --title",
    "build-blog": "node ./scripts/build-blog.js --title"
  }
}